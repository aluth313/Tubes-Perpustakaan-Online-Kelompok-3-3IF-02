<%- include ('../../../../partials/admin/header') -%>

<title>Home</title>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Tambah Anggota</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Blank Page</li>
                    </ol>
                </div>
            </div>

        </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <div class="card-body">
        <form role="form" action="/admin/anggota/tambah" method="POST">
            <div class="row">
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>NIP</label>
                        <input type="text" class="form-control" name="nipAnggota" placeholder="Enter ...">
                    </div>
                </div>
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>NIK</label>
                        <input type="text" class="form-control" name="nikAnggota" placeholder="Enter ...">
                    </div>
                </div>
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>Email</label>
                        <input type="email" class="form-control" name="emailAnggota" placeholder="Enter ...">
                    </div>
                </div>
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>Nama</label>
                        <input type="text" class="form-control" name="namaAnggota" placeholder="Enter ...">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-7">
                    <!-- textarea -->
                    <div class="form-group">
                        <label>Alamat</label>
                        <textarea class="form-control" name="alamatAnggota" rows="3" placeholder="Enter ..."></textarea>
                    </div>
                </div>
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>Jenis Kelamin</label>
                        <input type="text" class="form-control" name="jkAnggota" placeholder="Enter ...">
                    </div>
                </div>
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>Kota Lahir</label>
                        <input type="text" class="form-control" name="klAnggota" placeholder="Enter ...">
                    </div>
                </div>
                <div class="col-sm-7">
                    <!-- text input -->
                    <div class="form-group">
                        <label>Tanggal Lahir</label>
                        <input type="date" class="form-control" name="tlAnggota" placeholder="Enter ...">
                    </div>
                </div>
                <div class="col-sm-7">
                    <div class="card-footer">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                </div>

        </form>
    </div>

    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<!-- import data footer -->

<%- include ('../../../../partials/admin/footer') -%>