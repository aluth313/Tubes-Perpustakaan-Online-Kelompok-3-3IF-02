<%- include ('../../../../partials/user/header') -%>
<title>Home</title>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <div class="container">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Profile Anggota</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item active">Profile</li>
                    </ol>
                </div>
            </div>

        </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <div class="card">
        <table>
            <tr>
                <td style="padding-left: 10px">NIK</td>
                <td>:</td>
                <td><%= anggota.nik %></td>
            </tr>
            <tr>
                <td style="padding-left: 10px">Nama</td>
                <td>:</td>
                <td><%= anggota.nama %></td>
            </tr>
            <tr>
                <td style="padding-left: 10px">Alamat</td>
                <td>:</td>
                <td><%= anggota.alamat %></td>
            </tr>
            <tr>
                <td style="padding-left: 10px">Jenis Kelamin</td>
                <td>:</td>
                <td><%= anggota.jk %></td>
            </tr>
            <tr>
                <td style="padding-left: 10px">Kota Lahir</td>
                <td>:</td>
                <td><%= anggota.kota_lahir %></td>
            </tr>
            <tr>
                <td style="padding-left: 10px">Tanggal Lahir</td>
                <td>:</td>
                <td><%= anggota.tgl_lahir %></td>
            </tr>
            <tr>
                <td style="padding-left: 10px">Email</td>
                <td>:</td>
                <td><%= anggota.user.email %></td>
            </tr>
        </table>
    </div>
    <!-- /.content -->
</div>
</div>
<!-- /.content-wrapper -->
<!-- import data footer -->
<%- include ('../../../../partials/user/footer') -%>
<script type="text/javascript">
    $(document).ready(function() {
        $('#profile').addClass('active')
    })
</script>